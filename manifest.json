{
  "manifest_version": 2,
  "name": "AccessUpstate",
  "version": "1.2.0.1",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "content_scripts": [{
    "js":["js/preloader.js", "js/modifyCSSvalues.js", "js/weather.js", "js/clock.js", "js/uptodate.js"],
    "matches": [ "https://*/*"]
    }],
    "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
  "browser_action" : {
    "default_icon": "img/upstate-seal16.png"
  },
  "icons": {"16": "img/upstate-seal16.png",
            "48": "img/upstate-seal48.png",
            "128": "img/upstate-seal.png"},
  "description": "Chrome Tab Extension for SUNY Upstate Students"
}
